{
    "version": 2,
    "builds": [
        {
            "src": "client/package.json",
            "use": "@vercel/next"
        },
        {
            "src": "api/server.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/server.py"
        },
        {
            "src": "/(.*)",
            "dest": "/client/$1"
        }
    ]
}